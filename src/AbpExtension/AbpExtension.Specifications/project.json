    "iconUrl": "http://www.aspnetboilerplate.com/images/abp_nupkg.png",
    "projectUrl": "https://github.com/ZhaoRd/AbpExtension/wiki",
    "licenseUrl": "https://github.com/ZhaoRd/AbpExtension/blob/master/LICENSE",
    "requireLicenseAcceptance": false,
    "repository": {
      "type": "git",
      "url": "https://github.com/ZhaoRd/AbpExtension.git"
    }
  }


