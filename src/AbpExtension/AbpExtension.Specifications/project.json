{
  "version": "1.2.1-*",

  "dependencies": {
    "Abp": "1.2.1-*"
  },

  "frameworks": {
    "net452": {
      "frameworkAssemblies": {
        "System.Configuration": "",
        "System.Runtime": {
          "type": "build"
        },
        "System.Threading.Tasks": {
          "type": "build"
        }
      }
    }
